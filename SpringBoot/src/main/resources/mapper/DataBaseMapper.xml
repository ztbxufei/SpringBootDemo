<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.springboot.jingfei.SpringBoot.dao.DataBaseDao">
    <insert id="insert" parameterType="java.util.Map">
        insert into ${tableName} (
        <foreach collection="database" index="key" item="val" separator=",">
            <if test="val != null">
                ${key}
            </if>
        </foreach>
        )
        values(
        <foreach collection="database" index="key" item="val" separator=",">
            <if test="val != null">
                #{val}
            </if>
        </foreach>
        )
    </insert>

    <delete id="delete" parameterType="java.util.Map">
        delete from ${tableName} where ${primaryKey} = #{primaryValue}
    </delete>

    <select id="selectList" parameterType="java.util.Map" resultType="java.util.Map">
        select * from ${tableName} where ${primaryKey} = #{primaryValue}
        <if test="orderField != null and orderField != ''">
            order by ${orderField} ${orderDesc}
        </if>
    </select>

    <select id="select" parameterType="java.util.Map" resultType="java.util.Map">
        select * from ${tableName} where ${primaryKey} = #{primaryValue}
    </select>

    <update id="update" parameterType="java.util.Map">
        update ${tableName} set
        <foreach collection="database" index="key" item="val" separator=",">
            <if test=" key != 'null' ">
                ${key}=#{val}
            </if>
        </foreach>
        where ${primaryKey} = #{primaryValue}
    </update>
</mapper>